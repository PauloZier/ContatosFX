               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <TextField fx:id="txId" alignment="CENTER_RIGHT" editable="false" layoutX="31.0" layoutY="77.0" prefHeight="29.0" prefWidth="120.0" promptText="Id">
